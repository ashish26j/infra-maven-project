pipeline {
    
    agent any
    
    parameters {
        //string(defaultValue: "", description: 'Build number', name: 'BUILDNUMALLIEN')
        string(defaultValue: "", description: 'Branch name', name: 'BRANCH_NAME')
        string(defaultValue: "", description: 'Version Number', name: 'VERSION_NUM')
    }
    
    
    
   
    stages{
        stage ('test') {
            steps{
                script{
                    println params.BRANCH_NAME + "====" + params.VERSION_NUM
                }
            }
        }
    }
}

